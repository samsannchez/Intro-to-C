#include <stdio.h>
int fibonacci(int n);

int main()
{
    int number, i;
    printf("How many digits do you want?: ");
    scanf("%i", &number);
    for(i=1;i<=number;i++){
    printf("%i", fibonacci(i));
    
}

    return 0;
}

int fibonacci(int n){
if(n==1 || n==0){
    return n;
}
else{
    return (fibonacci(n-1)+fibonacci(n-2));
}
    
    
}
